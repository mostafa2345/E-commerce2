{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "API_KEY": "@api_key"
  },
  "functions": {
    "server.js": {
      "memory": 512,
      "maxDuration": 10
    }
  }
}
